{
  "name": "uxmanager",
  "version": "1.0.4",
  "description": "Vanjaro Preset",
  "main": "Scripts/uxmanager.min.js",
  "scripts": {
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "build": "webpack --config webpack.config.js",
    "start": "webpack-dev-server --open --progress --colors & npm run build:css -- -w",
    "build:css": "node-sass src/style/main.scss dist/grapesjs-preset-webpage.min.css --output-style compressed"
  },
  "author": "Vanjaro",
  "license": "MIT",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@babel/preset-env": "7.14.1",
    "babel-core": "6.26.3",
    "babel-loader": "8.2.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "7.26.0",
    "html-webpack-plugin": "4.5.2",
    "node-sass": "6.0.0",
    "webpack": "4.46.0",
    "webpack-cli": "4.7.0",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "grapesjs": "0.17.3",
    "grapesjs-style-filter": "0.1.3",
    "grapesjs-touch": "0.1.1",
    "grapesjs-tui-image-editor": "0.1.3",
    "jspanel4": "4.11.4",
    "popper.js": "1.16.1",
    "underscore": "1.13.1",
    "underscore.string": "3.3.5",
    "grapick": "0.1.13"
  }
}
